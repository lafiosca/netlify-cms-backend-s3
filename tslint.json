{
	"defaultSeverity": "error",
	"extends": [
		"tslint-config-airbnb"
	],
	"jsRules": {},
	"rules": {
		"indent": [true, "tabs"],
		"ter-indent": [true, "tab"],
		"ter-no-tabs": [false],
		"ter-arrow-parens": [2, "always"],
		"no-console": [false],
		"import-name": [true, {
			"react": "React"
		}]
	},
	"rulesDirectory": []
}
